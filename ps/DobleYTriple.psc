Proceso DobleYTriple
	Definir n,doble,triple Como Real;
	
	Escribir "<<<<<< Doble y triple de un número >>>";
	Escribir "Digite el número"; 
	Leer n;
	//Son dos variables de salida, por ende se deben crear dos comandos de salida. 
	doble<-n*2;
	triple<-n*3;
	//Dos comandos de salida. 
	Escribir "*** El doble del número ",n " es: ",doble;
	Escribir "*** El triple del número ",n " es: ",triple;
	
FinProceso
